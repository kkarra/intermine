<project type="bio">
  <property name="target.model" value="genomic"/>
  <property name="source.location" location="../bio/sources/"/>
  <property name="source.location" location="../bio/sources/sgd"/>
  <property name="common.os.prefix" value="common"/>
  <property name="intermine.properties.file" value="yeastmine.properties"/>
  <property name="default.intermine.properties.file" location="../default.intermine.integrate.properties"/>
  <sources>
    <source name="sgd" type="sgd" >      
    	<property name="source.db.name" value="sgd"/>
        <property name="dataSourceName" value="SGD"/>
        <property name="converter.class" value="org.intermine.bio.dataconversion.SgdConverter"/>
    </source>

    <source name="uniprot" type="uniprot">
         <property name="uniprot.organisms" value="4932 559292"/>
         <property name="src.data.dir" location="/Users/kkarra/share/gomine/uniprot/current/split"/>
         <property name="createinterpro" value="true"/>
    </source> 

     <source name="spell-expression" type="spell-expression">
        <property name="source.db.name" value="yeast"/>
        <property name="dataSourceName" value="yeast"/>
        <property name="converter.class" value="org.intermine.bio.dataconversion.SpellExpressionConverter"/>
   </source>

  <source name="treefam" type="treefam">
      <property name="src.data.dir" location="/Users/kkarra/share/intermine/data/yeast_orthologs/treefam"/>
      <property name="src.data.dir.includes" value="ortholog.txt.table"/>
      <property name="geneFile" value="/Users/kkarra/share/intermine/data/yeast_orthologs/treefam/genes.txt.table"/>
      <property name="treefam.organisms" value="4932"/> 
      <property name="treefam.homologues" value="7227 6239 7165 9606 10090 10116 7955"/>
   </source>

    <source name="ensembl-compara" type="ensembl-compara">
      <property name="src.data.dir" location="/Users/kkarra/share/intermine/data/yeast_orthologs/ensembl_compara"/>
      <property name="ensemblcompara.organisms" value="4932"/>
      <property name="ensemblcompara.homologues" value="9606 7227 6239 10090 10116 7955"/>
    </source>

    <source name="panther" type="panther">
      <property name="src.data.dir" location="/Users/kkarra/share/intermine/data/yeast_orthologs/panther"/>
      <property name="panther.organisms" value="4932"/>
      <property name="panther.homologues" value="9606 7227 6239 10090 10116 7955"/>
    </source>

    <source name="go" type="go">
      <property name="src.data.file" location="/Users/kkarra/share/intermine/data/gene_ontology_edit.obo"/>
    </source>

     <source name="go-slim" type="go-slim">
      <property name="src.data.file" location="/Users/kkarra/share/intermine/data/goslim_yeast.obo"/>
    </source>

    <source name="go-annotation" type="go-annotation">
      <property name="ontologyfile" location="/Users/kkarra/share/intermine/data/gene_ontology_edit.obo"/>
      <property name="src.data.dir" location="/Users/kkarra/share/intermine/data"/>
      <property name="src.data.dir.includes" value="ym_gene_association.sgd"/>
    </source>
   
    <source name="update-publications" type="update-publications">
      <property name="src.data.file" location="build/publications.xml"/>
    </source>
    
     <source name="entrez-organism" type="entrez-organism">
      <property name="src.data.file" location="build/organisms.xml"/>
    </source>
    
     <source name="so" type="so">
      <property name="src.data.file" location="../../bio/sources/so/so.obo"/>
    </source>

  </sources>

  <post-processing>
    <post-process name="create-references"/>
    <post-process name="do-sources"/>
    <post-process name="create-intergenic-region-features"/>
    <post-process name="transfer-sequences"/> 
    <post-process name="create-bioseg-location-index"/>
    <post-process name="create-overlap-view"/>
    <post-process name="create-gene-flanking-features"/>
    <post-process name="create-search-index"/>
    <post-process name="create-attribute-indexes"/>
    <post-process name="summarise-objectstore"/>
    <post-process name="create-autocomplete-index"/>
  </post-processing>

</project>

